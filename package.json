{
  "name": "didibudget-frontend",
  "version": "0.11.0",
  "description": "didibudget-frontend",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.3.20",
    "@reach/router": "1.3.4",
    "graphql": "15.5.1",
    "graphql-tag": "2.12.5",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.1",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.2.0",
    "react-scripts": "4.0.3",
    "react-toggle": "4.1.2",
    "react-widgets": "4.6.1",
    "react-widgets-moment": "4.0.30",
    "reactstrap": "8.9.0",
    "recharts": "2.0.9"
  },
  "scripts": {
    "lint": "eslint --ext .js ./src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "budget",
    "React",
    "GraphQL",
    "Apollo",
    "client"
  ],
  "author": "Dídac García (https://didaquis.github.io/)",
  "license": "MIT",
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "no-lenght"
    ],
    "rules": {
      "no-lenght/no-lenght": "error",
      "semi": [
        "warn",
        "never"
      ],
      "quotes": [
        "warn",
        "single"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "eslint-plugin-no-lenght": "1.0.2-0",
    "husky": "4.3.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && CI=true npm run test"
    }
  }
}
